<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="Login">
 
    <select id="SelectUser" resultType="com.example.demo.pc.comm.vo.UserInfo">;
        SELECT
			USER_IDXX            
			,SCRT_NUMB            
			,USDN_CODE            
			,EMPL_NUMB            
			,USER_NAME            
			,DEPT_CODE            
			,CUST_CODE            
			,USEX_YSNO            
			,REMK_100X            
			,INST_USID            
		FROM
			TM_USERXM
		WHERE USER_IDXX = #{USER_IDXX}
    </select>
 
</mapper>


